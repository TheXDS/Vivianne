<UserControl
    x:Class="TheXDS.Vivianne.Views.FeData4EditorView"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:gn="http://schemas.thexds.local/ganymede"
    xmlns:mcart="http://schemas.thexds.local/mcart"
    xmlns:ct="clr-namespace:TheXDS.Vivianne.Controls"
    xmlns:md4="clr-namespace:TheXDS.Vivianne.Models.Fe.Nfs4;assembly=VivLib"
    xmlns:md="clr-namespace:TheXDS.Vivianne.Models.Fe;assembly=VivLib"
    xmlns:st="clr-namespace:TheXDS.Vivianne.Resources.Strings.Views"
    xmlns:stc="clr-namespace:TheXDS.Vivianne.Resources.Strings;assembly=Vivianne.Common"
    xmlns:vm="clr-namespace:TheXDS.Vivianne.ViewModels.Fe;assembly=Vivianne.Common"
    xmlns:mdl="clr-namespace:TheXDS.Vivianne.Models;assembly=VivLib"
    xmlns:controls="clr-namespace:TheXDS.Vivianne.Controls"
    xmlns:viewmodels="clr-namespace:TheXDS.Vivianne.ViewModels;assembly=Vivianne.Common"
    xmlns:models="clr-namespace:TheXDS.Vivianne.Models;assembly=Vivianne.Common"
    xmlns:vc="clr-namespace:TheXDS.Vivianne.ValueConverters"
    xmlns:sco="clr-namespace:System.Collections.ObjectModel;assembly=System.ObjectModel"
    xmlns:sys="clr-namespace:System;assembly=netstandard"
    xmlns:hlp="clr-namespace:TheXDS.Vivianne.Helpers"
    xmlns:ui="http://schemas.lepo.co/wpfui/2022/xaml"
    xmlns:valueconverters="clr-namespace:TheXDS.Vivianne.ValueConverters"
    d:DataContext="{d:DesignInstance Type=vm:FeData4EditorViewModel}"
    mc:Ignorable="d"
    d:DesignWidth="600">
    <DockPanel>
        <controls:FileEditorCommandBar DockPanel.Dock="Top" Editor="{Binding}" ShowClose="False"/>
        <Grid Background="{ui:ThemeResource ControlSolidFillColorDefaultBrush}">
            <ScrollViewer>
                <ScrollViewer.Resources>
                    <Style TargetType="ScrollViewer">
                        <Setter Property="VerticalScrollBarVisibility" Value="Auto"/>
                    </Style>
                    <Style TargetType="ui:TextBox" BasedOn="{StaticResource {x:Type ui:TextBox}}">
                        <Setter Property="Margin" Value="5"/>
                    </Style>
                    <Style TargetType="ui:NumberBox" BasedOn="{StaticResource {x:Type ui:NumberBox}}">
                        <Setter Property="Margin" Value="5"/>
                    </Style>
                    <Style TargetType="UniformGrid">
                        <Setter Property="Margin" Value="5"/>
                        <Setter Property="VerticalAlignment" Value="Top"/>
                    </Style>
                    <Style TargetType="GroupBox">
                        <Setter Property="Margin" Value="5"/>
                    </Style>
                </ScrollViewer.Resources>
                <StackPanel Margin="10,10,0,10">
                    <Expander Header="{x:Static st:FeDataEditorView.BasicProperties}" IsExpanded="True">
                        <StackPanel Margin="5">
                            <ui:TextBox
                                hlp:Form.Label="{x:Static st:FeDataEditorView.CarName}"
                                Margin="5"
                                Text="{Binding State.CarName, UpdateSourceTrigger=PropertyChanged}"/>
                            <mcart:StretchyWrapPanel StretchProportionally="True">
                                <ui:TextBox
                                    hlp:Form.Label="{x:Static st:FeDataEditorView.CarId}"
                                    VerticalAlignment="Center"
                                    MaxLength="4"
                                    Margin="5"
                                    HorizontalAlignment="Stretch"
                                    Text="{Binding State.CarId, UpdateSourceTrigger=PropertyChanged}"/>
                                <ui:NumberBox
                                    hlp:Form.Label="{x:Static st:FeDataEditorView.SerialNumber}"
                                    VerticalAlignment="Center"
                                    HorizontalAlignment="Stretch"
                                    Margin="5"
                                    Minimum="0"
                                    Maximum="65535"
                                    Value="{Binding State.SerialNumber, UpdateSourceTrigger=PropertyChanged}"/>
                                <ComboBox
                                    hlp:Form.Label="{x:Static st:FeDataEditorView.CarClass}"
                                    Margin="5"
                                    IsEditable="False"
                                    SelectedItem="{Binding State.VehicleClass, UpdateSourceTrigger=PropertyChanged}"
                                    ItemsSource="{ct:EnumValProvider EnumType={x:Type md4:CarClass}}"/>
                                <ComboBox
                                    hlp:Form.Label="{x:Static st:FeDataEditorView.IsPolice}"
                                    Margin="5"
                                    IsEditable="False"
                                    SelectedValuePath="Value"
                                    DisplayMemberPath="Name"
                                    SelectedValue="{Binding State.PoliceFlag, UpdateSourceTrigger=PropertyChanged}"
                                    ItemsSource="{ct:NamedEnumValProvider EnumType={x:Type md4:PursuitFlag}}"/>
                                <CheckBox Margin="5" IsChecked="{Binding State.IsBonus}">
                                    <TextBlock Text="{x:Static st:FeDataEditorView.IsBonus}"/>
                                </CheckBox>
                                <CheckBox Margin="5" IsChecked="{Binding State.Upgradable}">
                                    <TextBlock Text="{x:Static st:FeDataEditorView.Upgradable}"/>
                                </CheckBox>
                                <CheckBox Margin="5" IsChecked="{Binding State.IsDlc, UpdateSourceTrigger=PropertyChanged}">
                                    <TextBlock Text="{x:Static st:FeDataEditorView.IsDLC}"/>
                                </CheckBox>
                                <ComboBox
                                    hlp:Form.Label="{x:Static st:FeDataEditorView.RoofType}"
                                    Margin="5"
                                    IsEditable="False"
                                    SelectedValuePath="Value"
                                    DisplayMemberPath="Name"
                                    SelectedValue="{Binding State.Roof, UpdateSourceTrigger=PropertyChanged}"
                                    ItemsSource="{ct:NamedEnumValProvider EnumType={x:Type md4:RoofFlag}}"/>
                                <ComboBox
                                    hlp:Form.Label="{x:Static st:FeDataEditorView.EngineLocation}"
                                    Margin="5"
                                    IsEditable="False"
                                    SelectedValuePath="Value"
                                    DisplayMemberPath="Name"
                                    SelectedValue="{Binding State.EngineLocation, UpdateSourceTrigger=PropertyChanged}"
                                    ItemsSource="{ct:NamedEnumValProvider EnumType={x:Type md:EngineLocation}}"/>
                            </mcart:StretchyWrapPanel>
                        </StackPanel>
                    </Expander>
                    <mcart:StretchyWrapPanel>
                        <Expander MinWidth="200" Header="{x:Static st:FeDataEditorView.Compare}" VerticalAlignment="Stretch" IsExpanded="True">
                            <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="3*"/>
                                    <ColumnDefinition/>
                                </Grid.ColumnDefinitions>
                                <Grid.RowDefinitions>
                                    <RowDefinition/>
                                    <RowDefinition/>
                                    <RowDefinition/>
                                    <RowDefinition/>
                                    <RowDefinition/>
                                    <RowDefinition/>
                                </Grid.RowDefinitions>
                                <TextBlock TextAlignment="Right" Margin="5" Text="{x:Static st:FeDataEditorView.Acceleration}"/>
                                <Slider VerticalAlignment="Center" Grid.Column="1" Maximum="20" Value="{Binding State.DefaultCompare.Acceleration}"/>
                                <TextBlock TextAlignment="Center" Margin="5" Grid.Column="2" Text="{Binding State.DefaultCompare.Acceleration, Mode=OneWay}"/>
                                <TextBlock TextAlignment="Right" Margin="5" Grid.Row="1" Text="{x:Static st:FeDataEditorView.TopSpeed}"/>
                                <Slider VerticalAlignment="Center" Grid.Row="1" Grid.Column="1" Maximum="20" Value="{Binding State.DefaultCompare.TopSpeed}"/>
                                <TextBlock TextAlignment="Center" Margin="5" Grid.Row="1" Grid.Column="2" Text="{Binding State.DefaultCompare.TopSpeed, Mode=OneWay}"/>
                                <TextBlock TextAlignment="Right" Margin="5" Grid.Row="2" Text="{x:Static st:FeDataEditorView.Handling}"/>
                                <Slider VerticalAlignment="Center" Grid.Row="2" Grid.Column="1" Maximum="20" Value="{Binding State.DefaultCompare.Handling}"/>
                                <TextBlock TextAlignment="Center" Margin="5" Grid.Row="2" Grid.Column="2" Text="{Binding State.DefaultCompare.Handling, Mode=OneWay}"/>
                                <TextBlock TextAlignment="Right" Margin="5" Grid.Row="3" Text="{x:Static st:FeDataEditorView.Braking}"/>
                                <Slider VerticalAlignment="Center" Grid.Row="3" Grid.Column="1" Maximum="20" Value="{Binding State.DefaultCompare.Braking}"/>
                                <TextBlock TextAlignment="Center" Margin="5" Grid.Row="3" Grid.Column="2" Text="{Binding State.DefaultCompare.Braking, Mode=OneWay}"/>
                                <TextBlock TextAlignment="Right" Margin="5" Grid.Row="4" Text="Overall"/>
                                <Slider VerticalAlignment="Center" Grid.Row="4" Grid.Column="1" Maximum="20" Value="{Binding State.DefaultCompare.Overall}"/>
                                <TextBlock TextAlignment="Center" Margin="5" Grid.Row="4" Grid.Column="2" Text="{Binding State.DefaultCompare.Overall, Mode=OneWay}"/>
                                <ui:NumberBox Grid.ColumnSpan="3" Grid.Row="5" hlp:Form.Label="Price" Value="{Binding State.DefaultCompare.Price, UpdateSourceTrigger=PropertyChanged}"/>
                            </Grid>
                        </Expander>
                        <Expander MinWidth="200" Header="{x:Static st:FeDataEditorView.CompareUpgrade1}" VerticalAlignment="Stretch" IsExpanded="True">
                            <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="3*"/>
                                    <ColumnDefinition/>
                                </Grid.ColumnDefinitions>
                                <Grid.RowDefinitions>
                                    <RowDefinition/>
                                    <RowDefinition/>
                                    <RowDefinition/>
                                    <RowDefinition/>
                                    <RowDefinition/>
                                    <RowDefinition/>
                                </Grid.RowDefinitions>
                                <TextBlock TextAlignment="Right" Margin="5" Text="{x:Static st:FeDataEditorView.Acceleration}"/>
                                <Slider VerticalAlignment="Center" Grid.Column="1" Maximum="20" Value="{Binding State.CompareUpg1.Acceleration}"/>
                                <TextBlock TextAlignment="Center" Margin="5" Grid.Column="2" Text="{Binding State.CompareUpg1.Acceleration, Mode=OneWay}"/>
                                <TextBlock TextAlignment="Right" Margin="5" Grid.Row="1" Text="{x:Static st:FeDataEditorView.TopSpeed}"/>
                                <Slider VerticalAlignment="Center" Grid.Row="1" Grid.Column="1" Maximum="20" Value="{Binding State.CompareUpg1.TopSpeed}"/>
                                <TextBlock TextAlignment="Center" Margin="5" Grid.Row="1" Grid.Column="2" Text="{Binding State.CompareUpg1.TopSpeed, Mode=OneWay}"/>
                                <TextBlock TextAlignment="Right" Margin="5" Grid.Row="2" Text="{x:Static st:FeDataEditorView.Handling}"/>
                                <Slider VerticalAlignment="Center" Grid.Row="2" Grid.Column="1" Maximum="20" Value="{Binding State.CompareUpg1.Handling}"/>
                                <TextBlock TextAlignment="Center" Margin="5" Grid.Row="2" Grid.Column="2" Text="{Binding State.CompareUpg1.Handling, Mode=OneWay}"/>
                                <TextBlock TextAlignment="Right" Margin="5" Grid.Row="3" Text="{x:Static st:FeDataEditorView.Braking}"/>
                                <Slider VerticalAlignment="Center" Grid.Row="3" Grid.Column="1" Maximum="20" Value="{Binding State.CompareUpg1.Braking}"/>
                                <TextBlock TextAlignment="Center" Margin="5" Grid.Row="3" Grid.Column="2" Text="{Binding State.CompareUpg1.Braking, Mode=OneWay}"/>
                                <TextBlock TextAlignment="Right" Margin="5" Grid.Row="4" Text="Overall"/>
                                <Slider VerticalAlignment="Center" Grid.Row="4" Grid.Column="1" Maximum="20" Value="{Binding State.CompareUpg1.Overall}"/>
                                <TextBlock TextAlignment="Center" Margin="5" Grid.Row="4" Grid.Column="2" Text="{Binding State.CompareUpg1.Overall, Mode=OneWay}"/>
                                <ui:NumberBox Grid.ColumnSpan="3" Grid.Row="5" hlp:Form.Label="Price" Value="{Binding State.CompareUpg1.Price, UpdateSourceTrigger=PropertyChanged}"/>
                            </Grid>
                        </Expander>
                        <Expander MinWidth="200" Header="{x:Static st:FeDataEditorView.CompareUpgrade2}" VerticalAlignment="Stretch" IsExpanded="True">
                            <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="3*"/>
                                    <ColumnDefinition/>
                                </Grid.ColumnDefinitions>
                                <Grid.RowDefinitions>
                                    <RowDefinition/>
                                    <RowDefinition/>
                                    <RowDefinition/>
                                    <RowDefinition/>
                                    <RowDefinition/>
                                    <RowDefinition/>
                                </Grid.RowDefinitions>
                                <TextBlock TextAlignment="Right" Margin="5" Text="{x:Static st:FeDataEditorView.Acceleration}"/>
                                <Slider VerticalAlignment="Center" Grid.Column="1" Maximum="20" Value="{Binding State.CompareUpg2.Acceleration}"/>
                                <TextBlock TextAlignment="Center" Margin="5" Grid.Column="2" Text="{Binding State.CompareUpg2.Acceleration, Mode=OneWay}"/>
                                <TextBlock TextAlignment="Right" Margin="5" Grid.Row="1" Text="{x:Static st:FeDataEditorView.TopSpeed}"/>
                                <Slider VerticalAlignment="Center" Grid.Row="1" Grid.Column="1" Maximum="20" Value="{Binding State.CompareUpg2.TopSpeed}"/>
                                <TextBlock TextAlignment="Center" Margin="5" Grid.Row="1" Grid.Column="2" Text="{Binding State.CompareUpg2.TopSpeed, Mode=OneWay}"/>
                                <TextBlock TextAlignment="Right" Margin="5" Grid.Row="2" Text="{x:Static st:FeDataEditorView.Handling}"/>
                                <Slider VerticalAlignment="Center" Grid.Row="2" Grid.Column="1" Maximum="20" Value="{Binding State.CompareUpg2.Handling}"/>
                                <TextBlock TextAlignment="Center" Margin="5" Grid.Row="2" Grid.Column="2" Text="{Binding State.CompareUpg2.Handling, Mode=OneWay}"/>
                                <TextBlock TextAlignment="Right" Margin="5" Grid.Row="3" Text="{x:Static st:FeDataEditorView.Braking}"/>
                                <Slider VerticalAlignment="Center" Grid.Row="3" Grid.Column="1" Maximum="20" Value="{Binding State.CompareUpg2.Braking}"/>
                                <TextBlock TextAlignment="Center" Margin="5" Grid.Row="3" Grid.Column="2" Text="{Binding State.CompareUpg2.Braking, Mode=OneWay}"/>
                                <TextBlock TextAlignment="Right" Margin="5" Grid.Row="4" Text="Overall"/>
                                <Slider VerticalAlignment="Center" Grid.Row="4" Grid.Column="1" Maximum="20" Value="{Binding State.CompareUpg2.Overall}"/>
                                <TextBlock TextAlignment="Center" Margin="5" Grid.Row="4" Grid.Column="2" Text="{Binding State.CompareUpg2.Overall, Mode=OneWay}"/>
                                <ui:NumberBox Grid.ColumnSpan="3" Grid.Row="5" hlp:Form.Label="Price" Value="{Binding State.CompareUpg2.Price, UpdateSourceTrigger=PropertyChanged}"/>
                            </Grid>
                        </Expander>
                        <Expander MinWidth="200" Header="{x:Static st:FeDataEditorView.CompareUpgrade3}" VerticalAlignment="Stretch" IsExpanded="True">
                            <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="3*"/>
                                    <ColumnDefinition/>
                                </Grid.ColumnDefinitions>
                                <Grid.RowDefinitions>
                                    <RowDefinition/>
                                    <RowDefinition/>
                                    <RowDefinition/>
                                    <RowDefinition/>
                                    <RowDefinition/>
                                    <RowDefinition/>
                                </Grid.RowDefinitions>
                                <TextBlock TextAlignment="Right" Margin="5" Text="{x:Static st:FeDataEditorView.Acceleration}"/>
                                <Slider VerticalAlignment="Center" Grid.Column="1" Maximum="20" Value="{Binding State.CompareUpg3.Acceleration}"/>
                                <TextBlock TextAlignment="Center" Margin="5" Grid.Column="2" Text="{Binding State.CompareUpg3.Acceleration, Mode=OneWay}"/>
                                <TextBlock TextAlignment="Right" Margin="5" Grid.Row="1" Text="{x:Static st:FeDataEditorView.TopSpeed}"/>
                                <Slider VerticalAlignment="Center" Grid.Row="1" Grid.Column="1" Maximum="20" Value="{Binding State.CompareUpg3.TopSpeed}"/>
                                <TextBlock TextAlignment="Center" Margin="5" Grid.Row="1" Grid.Column="2" Text="{Binding State.CompareUpg3.TopSpeed, Mode=OneWay}"/>
                                <TextBlock TextAlignment="Right" Margin="5" Grid.Row="2" Text="{x:Static st:FeDataEditorView.Handling}"/>
                                <Slider VerticalAlignment="Center" Grid.Row="2" Grid.Column="1" Maximum="20" Value="{Binding State.CompareUpg3.Handling}"/>
                                <TextBlock TextAlignment="Center" Margin="5" Grid.Row="2" Grid.Column="2" Text="{Binding State.CompareUpg3.Handling, Mode=OneWay}"/>
                                <TextBlock TextAlignment="Right" Margin="5" Grid.Row="3" Text="{x:Static st:FeDataEditorView.Braking}"/>
                                <Slider VerticalAlignment="Center" Grid.Row="3" Grid.Column="1" Maximum="20" Value="{Binding State.CompareUpg3.Braking}"/>
                                <TextBlock TextAlignment="Center" Margin="5" Grid.Row="3" Grid.Column="2" Text="{Binding State.CompareUpg3.Braking, Mode=OneWay}"/>
                                <TextBlock TextAlignment="Right" Margin="5" Grid.Row="4" Text="Overall"/>
                                <Slider VerticalAlignment="Center" Grid.Row="4" Grid.Column="1" Maximum="20" Value="{Binding State.CompareUpg3.Overall}"/>
                                <TextBlock TextAlignment="Center" Margin="5" Grid.Row="4" Grid.Column="2" Text="{Binding State.CompareUpg3.Overall, Mode=OneWay}"/>
                                <ui:NumberBox Grid.ColumnSpan="3" Grid.Row="5" hlp:Form.Label="Price" Value="{Binding State.CompareUpg3.Price, UpdateSourceTrigger=PropertyChanged}"/>
                            </Grid>
                        </Expander>
                    </mcart:StretchyWrapPanel>
                    <mcart:StretchyWrapPanel>
                        <mcart:StretchyWrapPanel.Resources>
                            <Style TargetType="ui:TextBox" BasedOn="{StaticResource {x:Type ui:TextBox}}">
                                <Setter Property="MinWidth" Value="200"/>
                            </Style>
                        </mcart:StretchyWrapPanel.Resources>
                        <Expander Header="{x:Static st:FeDataEditorView.General}" IsExpanded="True" VerticalAlignment="Stretch">
                            <mcart:StretchyWrapPanel>
                                <ui:TextBox hlp:Form.Label="{x:Static st:FeDataEditorView.Manufacturer}" Text="{Binding State.Manufacturer, UpdateSourceTrigger=PropertyChanged}"/>
                                <ui:TextBox hlp:Form.Label="{x:Static st:FeDataEditorView.Model}" Text="{Binding State.Model, UpdateSourceTrigger=PropertyChanged}"/>
                                <ui:TextBox hlp:Form.Label="{x:Static st:FeDataEditorView.Price}" Text="{Binding State.Price, UpdateSourceTrigger=PropertyChanged}"/>
                                <ui:TextBox hlp:Form.Label="{x:Static st:FeDataEditorView.Status}" Text="{Binding State.Status, UpdateSourceTrigger=PropertyChanged}"/>
                                <ui:TextBox hlp:Form.Label="{x:Static st:FeDataEditorView.Weight}" Text="{Binding State.Weight, UpdateSourceTrigger=PropertyChanged}"/>
                                <ui:TextBox hlp:Form.Label="{x:Static st:FeDataEditorView.WeightDistr}" Text="{Binding State.WeightDistribution, UpdateSourceTrigger=PropertyChanged}"/>
                                <ui:TextBox hlp:Form.Label="{x:Static st:FeDataEditorView.Width}" Text="{Binding State.Width, UpdateSourceTrigger=PropertyChanged}"/>
                                <ui:TextBox hlp:Form.Label="{x:Static st:FeDataEditorView.Height}" Text="{Binding State.Height, UpdateSourceTrigger=PropertyChanged}"/>
                                <ui:TextBox hlp:Form.Label="{x:Static st:FeDataEditorView.Length}" Text="{Binding State.Length, UpdateSourceTrigger=PropertyChanged}"/>
                            </mcart:StretchyWrapPanel>
                        </Expander>
                        <Expander Header="{x:Static st:FeDataEditorView.Performance}" IsExpanded="True" VerticalAlignment="Stretch">
                            <mcart:StretchyWrapPanel>
                                <ui:TextBox hlp:Form.Label="{x:Static st:FeDataEditorView.Engine}" Text="{Binding State.Engine, UpdateSourceTrigger=PropertyChanged}"/>
                                <ui:TextBox hlp:Form.Label="{x:Static st:FeDataEditorView.Displacement}" Text="{Binding State.Displacement, UpdateSourceTrigger=PropertyChanged}"/>
                                <ui:TextBox hlp:Form.Label="{x:Static st:FeDataEditorView.Power}" Text="{Binding State.Hp, UpdateSourceTrigger=PropertyChanged}"/>
                                <ui:TextBox hlp:Form.Label="{x:Static st:FeDataEditorView.Torque}" Text="{Binding State.Torque, UpdateSourceTrigger=PropertyChanged}"/>
                                <ui:TextBox hlp:Form.Label="{x:Static st:FeDataEditorView.MaxRpm}" Text="{Binding State.MaxEngineSpeed, UpdateSourceTrigger=PropertyChanged}"/>
                                <ui:TextBox hlp:Form.Label="{x:Static st:FeDataEditorView.Brakes}" Text="{Binding State.Brakes, UpdateSourceTrigger=PropertyChanged}"/>
                                <ui:TextBox hlp:Form.Label="{x:Static st:FeDataEditorView.Tires}" Text="{Binding State.Tires, UpdateSourceTrigger=PropertyChanged}"/>
                                <ui:TextBox hlp:Form.Label="{x:Static st:FeDataEditorView.DynamicStability}" Text="{Binding State.DynamicStability, UpdateSourceTrigger=PropertyChanged}"/>
                                <ui:TextBox hlp:Form.Label="{x:Static st:FeDataEditorView.TopSpeed}" Text="{Binding State.TopSpeed, UpdateSourceTrigger=PropertyChanged}"/>
                                <ui:TextBox hlp:Form.Label="{x:Static st:FeDataEditorView.Accel0To60}" Text="{Binding State.Accel0To60, UpdateSourceTrigger=PropertyChanged}"/>
                                <ui:TextBox hlp:Form.Label="{x:Static st:FeDataEditorView.Accel0To100}" Text="{Binding State.Accel0To100, UpdateSourceTrigger=PropertyChanged}"/>
                                <ui:TextBox hlp:Form.Label="{x:Static st:FeDataEditorView.Transmission}" Text="{Binding State.Transmission, UpdateSourceTrigger=PropertyChanged}"/>
                                <ui:TextBox hlp:Form.Label="{x:Static st:FeDataEditorView.Gearbox}" Text="{Binding State.Gearbox, UpdateSourceTrigger=PropertyChanged}"/>
                            </mcart:StretchyWrapPanel>
                        </Expander>
                        <Expander Header="{x:Static st:FeDataEditorView.History}" IsExpanded="True" VerticalAlignment="Stretch">
                            <mcart:StretchyWrapPanel>
                                <ui:TextBox hlp:Form.Label="{x:Static st:FeDataEditorView.History1}" Text="{Binding State.History1, UpdateSourceTrigger=PropertyChanged}"/>
                                <ui:TextBox hlp:Form.Label="{x:Static st:FeDataEditorView.History2}" Text="{Binding State.History2, UpdateSourceTrigger=PropertyChanged}"/>
                                <ui:TextBox hlp:Form.Label="{x:Static st:FeDataEditorView.History3}" Text="{Binding State.History3, UpdateSourceTrigger=PropertyChanged}"/>
                                <ui:TextBox hlp:Form.Label="{x:Static st:FeDataEditorView.History4}" Text="{Binding State.History4, UpdateSourceTrigger=PropertyChanged}"/>
                                <ui:TextBox hlp:Form.Label="{x:Static st:FeDataEditorView.History5}" Text="{Binding State.History5, UpdateSourceTrigger=PropertyChanged}"/>
                                <ui:TextBox hlp:Form.Label="{x:Static st:FeDataEditorView.History6}" Text="{Binding State.History6, UpdateSourceTrigger=PropertyChanged}"/>
                                <ui:TextBox hlp:Form.Label="{x:Static st:FeDataEditorView.History7}" Text="{Binding State.History7, UpdateSourceTrigger=PropertyChanged}"/>
                                <ui:TextBox hlp:Form.Label="{x:Static st:FeDataEditorView.History8}" Text="{Binding State.History8, UpdateSourceTrigger=PropertyChanged}"/>
                            </mcart:StretchyWrapPanel>
                        </Expander>
                        <Expander Header="{x:Static st:FeDataEditorView.Colors}" IsExpanded="True" VerticalAlignment="Stretch">
                            <mcart:StretchyWrapPanel>
                                <mcart:StretchyWrapPanel.Resources>
                                    <valueconverters:DrawingColorToBrushConverter x:Key="dc2bc"/>
                                </mcart:StretchyWrapPanel.Resources>
                                <ui:TextBox hlp:Form.Label="{x:Static st:FeDataEditorView.Color1}" hlp:Form.Fce4ColorPreview="{Binding State.PreviewFceColorTable[0], Mode=OneWay}" Text="{Binding State.Color1, UpdateSourceTrigger=PropertyChanged}"/>
                                <ui:TextBox hlp:Form.Label="{x:Static st:FeDataEditorView.Color2}" hlp:Form.Fce4ColorPreview="{Binding State.PreviewFceColorTable[1], Mode=OneWay}" Text="{Binding State.Color2, UpdateSourceTrigger=PropertyChanged}"/>
                                <ui:TextBox hlp:Form.Label="{x:Static st:FeDataEditorView.Color3}" hlp:Form.Fce4ColorPreview="{Binding State.PreviewFceColorTable[2], Mode=OneWay}" Text="{Binding State.Color3, UpdateSourceTrigger=PropertyChanged}"/>
                                <ui:TextBox hlp:Form.Label="{x:Static st:FeDataEditorView.Color4}" hlp:Form.Fce4ColorPreview="{Binding State.PreviewFceColorTable[3], Mode=OneWay}" Text="{Binding State.Color4, UpdateSourceTrigger=PropertyChanged}"/>
                                <ui:TextBox hlp:Form.Label="{x:Static st:FeDataEditorView.Color5}" hlp:Form.Fce4ColorPreview="{Binding State.PreviewFceColorTable[4], Mode=OneWay}" Text="{Binding State.Color5, UpdateSourceTrigger=PropertyChanged}"/>
                                <ui:TextBox hlp:Form.Label="{x:Static st:FeDataEditorView.Color6}" hlp:Form.Fce4ColorPreview="{Binding State.PreviewFceColorTable[5], Mode=OneWay}" Text="{Binding State.Color6, UpdateSourceTrigger=PropertyChanged}"/>
                                <ui:TextBox hlp:Form.Label="{x:Static st:FeDataEditorView.Color7}" hlp:Form.Fce4ColorPreview="{Binding State.PreviewFceColorTable[6], Mode=OneWay}" Text="{Binding State.Color7, UpdateSourceTrigger=PropertyChanged}"/>
                                <ui:TextBox hlp:Form.Label="{x:Static st:FeDataEditorView.Color8}" hlp:Form.Fce4ColorPreview="{Binding State.PreviewFceColorTable[7], Mode=OneWay}" Text="{Binding State.Color8, UpdateSourceTrigger=PropertyChanged}"/>
                                <ui:TextBox hlp:Form.Label="{x:Static st:FeDataEditorView.Color9}" hlp:Form.Fce4ColorPreview="{Binding State.PreviewFceColorTable[8], Mode=OneWay}" Text="{Binding State.Color9, UpdateSourceTrigger=PropertyChanged}"/>
                                <ui:TextBox hlp:Form.Label="{x:Static st:FeDataEditorView.Color10}" hlp:Form.Fce4ColorPreview="{Binding State.PreviewFceColorTable[9], Mode=OneWay}" Text="{Binding State.Color10, UpdateSourceTrigger=PropertyChanged}"/>
                            </mcart:StretchyWrapPanel>
                        </Expander>
                    </mcart:StretchyWrapPanel>
                </StackPanel>
            </ScrollViewer>
        </Grid>
    </DockPanel>
</UserControl>
