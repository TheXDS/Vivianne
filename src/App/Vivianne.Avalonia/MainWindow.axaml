<Window
    xmlns="https://github.com/avaloniaui"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:gn="using:TheXDS.Ganymede.Controls"
	xmlns:gn2="using:TheXDS.Ganymede.Markup"
    xmlns:vm="clr-namespace:TheXDS.Vivianne.ViewModels;assembly=Vivianne.Common"
    mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
    x:Class="Vivianne.Avalonia.MainWindow"
	TransparencyLevelHint="AcrylicBlur"
    Title="Vivianne.Avalonia">	
    <gn:NavigationHost
		DialogService="{gn2:NavDialogService}"
		Navigator="{gn2:NavService Home={x:Type vm:StartupViewModel}}"
		VisualResolver="{gn2:ConventionResolver}"/>
</Window>
